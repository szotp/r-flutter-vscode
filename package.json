{
  "name": "r-flutter-vscode",
  "displayName": "r-flutter-vscode",
  "description": "VSCode helpers for r_flutter package",
  "version": "0.1.0",
  "publisher": "szotp",
  "repository": "https://github.com/szotp/r-flutter-vscode",
  "engines": {
    "vscode": "^1.32.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:dart"
  ],
  "license": "MIT",
  "main": "./out/extension.js",
  "contributes": {
    "commands": [],
    "languages": [
      {
        "id": "json",
        "extensions": [
          "arb"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "dependencies": {
    "@types/js-yaml": "^3.12.1",
    "google-translate": "^2.2.0",
    "js-yaml": "^3.13.1",
    "locale-code": "^2.0.2",
    "rtl-detect": "^1.0.2"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.10",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2",
    "vscode": "^1.1.34"
  },
  "galleryBanner": {
    "color": "#ed8a00",
    "theme": "dark"
  }
}